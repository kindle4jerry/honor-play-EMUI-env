<?xml version="1.0" encoding="UTF-8"?>
<SBLConfig>
    <LightSensorRateMills>300</LightSensorRateMills><!-- 传感器速率 -->
    <ApicalADLevel>128</ApicalADLevel><!-- SBL默认级别 -->
    <SceneRecognition>
        <SceneVideo>true</SceneVideo>
        <SceneGallery>false</SceneGallery>
        <SceneCamera>true</SceneCamera>
    </SceneRecognition><!-- 哪个应用程序为所有环境光打开 -->

    <OutDoorThreshold>1000</OutDoorThreshold><!-- 打开SBL lux阈值，lux diff better>=500lux用于打开和关闭 -->
    <InDoorThreshold>500</InDoorThreshold><!-- 关闭SBL lux阈值 -->
    <BrighenDebounceTime>3000</BrighenDebounceTime><!-- 打开SBL延迟时间 -->
    <DarkenDebounceTime>3000</DarkenDebounceTime><!-- 关闭SBL延迟时间 -->
    <BrightenLinePoints>
        <Point>0,15</Point>
        <Point>2,15</Point>
        <Point>10,19</Point>
        <Point>100,239</Point>
        <Point>500,439</Point>
        <Point>1000,989</Point>
        <Point>40000,989</Point>
    </BrightenLinePoints><!-- 照亮所有环境光线，不要改变 -->
    <DarkenLinePoints>
        <Point>0,1</Point>
        <Point>1,1</Point>
        <Point>15,15</Point>
        <Point>20,15</Point>
        <Point>85,80</Point>
        <Point>100,80</Point>
        <Point>420,400</Point>
        <Point>500,400</Point>
        <Point>600,500</Point>
        <Point>1000,500</Point>
        <Point>2000,1000</Point>
        <Point>40000,1000</Point>
    </DarkenLinePoints><!-- 所有环境光的暗线，不要改变 -->
</SBLConfig>